public class ReplaceString
{
    public static void main(String args[]){ 
        String s1="Sing, Ring,"; 
        String replaceString=s1.replace('S','B'); 
        String otherString=s1.replace('R', 'K');
        System.out.println(replaceString); 
        System.out.println(otherString);
    }
    
}
